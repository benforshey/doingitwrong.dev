		"eslint-plugin-svelte": "^2.31.1",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^4.2.0",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"nanoid": "^4.0.2",
		"open-props": "^1.5.10",
